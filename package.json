{
    "name": "lottie-web",
    "version": "5.12.2",
    "description": "After Effects plugin for exporting animations to SVG + JavaScript or canvas + JavaScript",
    "type": "module",
    "main": "./dist/lottie.umd.cjs",
    "module": "./dist/lottie.js",
    "files": ["dist"],
    "repository": {
        "type": "git",
        "url": "https://github.com/airbnb/lottie-web.git"
    },
    "sideEffects": false,
    "scripts": {
        "build": "bunx eslint ./player/js/**/* && rollup -c && node tasks/build_worker",
        "build_test": "node tasks/build.js",
        "lint": "bunx eslint ./player/js/**/*",
        "lint:fix": "bunx eslint ./player/js/**/* --fix",
        "test:create": "node test/index.js",
        "test:compare": "node test/index.js -s compare"
    },
    "keywords": [
        "animation",
        "canvas",
        "svg",
        "after effects",
        "plugin",
        "export"
    ],
    "devDependencies": {
        "@babel/core": "^7.25.8",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.25.8",
        "@eslint/compat": "^1.2.0",
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.12.0",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "cheerio": "^1.0.0-rc.11",
        "command-line-args": "^6.0.0",
        "eslint": "^9.12.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.31.0",
        "express": "^4.18.2",
        "globals": "^15.11.0",
        "pixelmatch": "^6.0.0",
        "pngjs": "^7.0.0",
        "puppeteer": "^20.5.0",
        "rollup": "^4.24.0",
        "rollup-plugin-terser": "^7.0.2",
        "uglify-js": "^3.4.9",
        "vite": "^5.4.8",
        "watch": "^1.0.2"
    },
    "types": "./index.d.ts",
    "license": "MIT"
}
